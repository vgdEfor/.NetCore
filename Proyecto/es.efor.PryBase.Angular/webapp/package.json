{
  "name": "PryBase-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "postinstall": "ngcc",
    "build": "npm run build:dev",
    "build:dev": "./node_modules/.bin/ng build --watch",
    "build:dev-nowatch-stats": "ng build --stats-json",
    "build:prod": "npm run build:prod-aot",
    "build:prod-aot": "ng build --prod",
    "build:prod-jit": "ng build --prod --aot=false",
    "build:prod-jit-stats": "ng build --prod --aot=false --buildOptimizer=false --stats-json",
    "build:prod-aot-stats": "ng build --prod --stats-json",
    "swagger": "ng-openapi-gen --input http://localhost:5000/swagger/v1/swagger.json --output src/app/shared/api",
    "translate": "ngx-translate-extract --input ./src --output ./src/assets/i18n/es.json ./src/assets/i18n/en.json --format namespaced-json --key-as-default-value --sort",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "stats": "npm-run-all build:dev-nowatch-stats analyze",
    "stats:prod": "npm-run-all build:prod-aot-stats analyze",
    "analyze": "webpack-bundle-analyzer ../../es.efor.PryBase.MainGateway/wwwroot/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.4",
    "@angular/common": "~10.1.4",
    "@angular/compiler": "~10.1.4",
    "@angular/core": "~10.1.4",
    "@angular/forms": "~10.1.4",
    "@angular/platform-browser": "~10.1.4",
    "@angular/platform-browser-dynamic": "~10.1.4",
    "@angular/router": "~10.1.4",
    "@angular/service-worker": "~10.1.3",
    "@angular/youtube-player": "^9.2.4",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@ckeditor/ckeditor5-angular": "^1.2.3",
    "@ckeditor/ckeditor5-build-classic": "^19.0.2",
    "@ckeditor/ckeditor5-image": "^19.0.1",
    "@fortawesome/angular-fontawesome": "~0.7.0",
    "@fortawesome/fontawesome-svg-core": "~1.2.31",
    "@fortawesome/free-brands-svg-icons": "~5.15.0",
    "@fortawesome/free-regular-svg-icons": "~5.15.0",
    "@fortawesome/free-solid-svg-icons": "~5.15.0",
    "@ngx-translate/core": "~13.0.0",
    "@ngx-translate/http-loader": "~6.0.0",
    "ax-toolbox": "0.0.23",
    "bootstrap": "~4.5.2",
    "hammerjs": "^2.0.8",
    "leaflet": "^1.7.1",
    "moment": "^2.29.0",
    "ng-lazyload-image": "^8.0.1",
    "ngx-bootstrap": "~6.1.0",
    "ngx-device-detector": "^1.5.2",
    "ngx-owl-carousel-o": "^3.1.1",
    "ngx-toastr": "~13.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "xlsx": "^0.16.9",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/cli": "~10.1.3",
    "@angular/compiler-cli": "~10.1.3",
    "@biesbjerg/ngx-translate-extract": "^7.0.2",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/leaflet": "^1.5.17",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-openapi-gen": "^0.12.1",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "webpack-bundle-analyzer": "^3.9.0"
  }
}
